{
  "name": "@cervantes/decorators",
  "version": "1.0.0",
  "description": "",
  "sideEffects": false,
  "engines": {
    "node": "21"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx tshy --watch",
    "build": "npx tshy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "tshy": "^1.8.1"
  },
  "tshy": {
    "dialects": [
      "esm",
      "commonjs"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "source": "./src/index.ts",
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "type": "module",
  "dependencies": {
    "async-cache-dedupe": "^2.0.0"
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js"
}
